{
  "name": "agent-os-tools",
  "version": "1.0.0",
  "description": "Simple tools for Agent OS standards and lessons learned",
  "main": "compliance-checker.js",
  "scripts": {
    "check": "node compliance-checker.js",
    "watch": "node cursor-integration.js watch",
    "validate": "node cursor-integration.js check",
    "report": "node cursor-integration.js check && open .agent-os/dashboard/compliance-dashboard.html",
    "lint": "node -c",
    "format": "prettier --write ."
  },
  "keywords": [
    "standards",
    "lessons-learned",
    "cursor",
    "agent-os"
  ],
  "author": "Agent OS Team",
  "license": "MIT",
  "dependencies": {
    "chokidar": "^3.5.3",
    "glob": "^10.3.10"
  },
  "devDependencies": {
    "prettier": "^3.6.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
