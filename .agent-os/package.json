{
  "name": "@agent-os/toolkit",
  "version": "1.0.0",
  "description": "Agent-OS Development Toolkit",
  "type": "module",
  "scripts": {
    "generate": "tsx cli/generate.ts",
    "validate:mocks": "tsx scripts/validate-mocks.ts",
    "validate:contracts": "tsx scripts/validate-contracts.ts",
    "validate:deps": "tsx scripts/validate-dependencies.ts",
    "validate:all": "npm run validate:mocks && npm run validate:contracts && npm run validate:deps",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "build": "tsc",
    "clean": "rm -rf dist coverage",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@faker-js/faker": "^8.3.1",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "^20.10.5",
    "axios": "^1.7.0",
    "commander": "^11.1.0",
    "handlebars": "^4.7.8",
    "vitest": "^0.34.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/react": "^19.1.1",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "@vitest/coverage-c8": "^0.34.0",
    "@vitest/ui": "^0.34.0",
    "eslint": "^9.17.0",
    "jsdom": "^22.1.0",
    "prettier": "^3.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.5.3"
  },
  "files": [
    "dist",
    "cli",
    "config",
    "contracts",
    "dependencies",
    "hooks",
    "ide",
    "patterns",
    "scripts",
    "templates",
    "testing"
  ],
  "exports": {
    "./testing": "./dist/testing/index.js",
    "./contracts": "./dist/contracts/index.js",
    "./config": "./dist/config/index.js"
  },
  "bin": {
    "agent-os": "./cli/generate.js"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/agent-os/toolkit.git"
  },
  "keywords": [
    "agent-os",
    "development",
    "toolkit",
    "testing",
    "code-generation"
  ],
  "author": "Agent-OS Team",
  "license": "MIT"
}